$items = array(
     "fff"=> array("lll"),
    "ddd" => array("lll"),
    "sss" => array("lll")
)
    
foreach($items as $itemKey => $itemValue)
{
    foreach($data as $areaKey => $areaValue)
    {
        array_push($data[$areaKey],array($itemKey=>array()));
    }
}


 $data = {
    "main": {
        "props": {
        "title": ""
      },
      "data: {
          item: {},
          item: {},
          item: {}
      }
      },
    "additional": {
        "props": {
        "title": ""
      },
      "data: {
          item: {},
          item: {},
          item: {}
      }
      },
    "technical": {
        "props": {
        "title": ""
      },
      "data: {
          item: {},
          item: {},
          item: {}
      }
      },
    "description": {
        "props": {
        "title": ""
      },
      "data: {
          item: {},
          item: {},
          item: {}
      }
      }
}



foreach ($items as $itemKey => $itemValue)
{
    foreach($itemValue as $paramKey => $paramValue)
    {
        $objToPushInto;

        switch ($paramValue.area) {
            case "details":
                {
                    if(! $data.main.data.itemKey['details'])
                        { 
                            $data.main.data.itemKey.push($areas['details']);
                        }  
                    $objToPushInto = $data.main.data.itemKey['details'].data; 
                }
                break;

            case "reviews":
            case "accessories":
            case "safety":
                {
                    if(! $data.additional.data.itemKey[$paramValue.area])
                        {
                            $data.additional.data.itemKey.push($areas[$paramValue.area])
                        }
                    $objToPushInto = $data.additional.data.itemKey[$paramValue.area].data          
                }
                break;

            case "technical":
            case "description":
                $objToPushInto = $data[$paramValue.area].data.itemKey;
                breake;
        }


        $objToPushInto.push($items[$paramKey]);
    }
}


foreach ($items as $itemKey => $itemValue)
{
    foreach($itemValue as $paramKey => $paramValue)
    {
        $objToPushInto;
        if($paramValue.area === 'details')
            {
                if(! $data.main.data.itemKey['details'])
                  { 
                       $data.main.data.itemKey.push($areas['details'])
                   }  
                $objToPushInto = $data.main.data.itemKey['details'].data
                  
            }
        else 
        {
            if($paramValue.area === ('reviews' || 'accessories' || 'safety'))
                {
                    if(! $data.additional.data.itemKey[$paramValue.area])
                       {
                            $data.additional.data.itemKey.push($areas[$paramValue.area]);
                       }
                    $objToPushInto = $data.additional.data.itemKey[$paramValue.area].data;
                       
                }
                else
                {
                    if($paramValue.area === ('technical' || 'description'))
                    {
                        $objToPushInto = $data[$paramValue.area].data.itemKey;
                    }
                }
        }

       $objToPushInto.push($items[$paramKey]);
    }
}



